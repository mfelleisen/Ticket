#!/bin/bash 

XSTRESS=../Benchmark/xstress-trace-no-load.out

echo "swap"
pushd ../Common/
cp player-interface-trace-no-load.rkt player-interface.rkt
popd

echo "making"
raco make xbench 
if [ ! -f $XSTRESS ] ; then
    echo "start: " $XSTRESS > $XSTRESS ;
fi 

gtime raco test ./xbench 2>>  $XSTRESS 

echo "swap back"
pushd ../Common/
git checkout player-interface.rkt 
popd

